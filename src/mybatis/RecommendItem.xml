<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="RecommendItem">


<select id="selectlistItem" resultType="org.shoppingMall.vo.RecommendItemVo">
SELECT * FROM RECOMMENDITEM
</select>

<insert id="insertItem" parameterType="org.shoppingMall.vo.RecommendItemVo">
	INSERT INTO RECOMMENDITEM VALUES (#{name},#{price},#{coverfile})
</insert>

<delete id="deleteItem" parameterType="String">
DELETE FROM RECOMMENDITEM 
WHERE coverfile = #{coverfile}
</delete>
 </mapper>
 
 
 
 
 
 